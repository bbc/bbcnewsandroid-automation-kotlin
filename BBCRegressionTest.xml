<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="BBCNewsSuite">
    <listeners>
    <listener class-name="com.bbcnews.automation.testutils.Listeners.RegressionTestListeners" />
    </listeners>
    <test name="BBCNewsRegressionTest" enabled="true">
        <groups>
            <run>
                <exclude name="ignoreTest"></exclude>
            </run>
        </groups>
        <classes>
            <class name="com.bbcnews.automation.scripts.BBCNewsReleaseBuildRegressionTest" />
        </classes>
    </test>
</suite> <!-- Suite -->
